      <% @sintomas.each do |x| %>
          <tr>
            <td><%= x.nome %></td>
          </tr>
      <% end %>
    </table>
    <form action="/<%= @acess %>/deleteTranstornoNaoSolucionado" method="post" onsubmit="return confirm('Você tem certeza que deseja deletar?');">
